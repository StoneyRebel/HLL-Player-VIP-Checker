{
  "name": "hell-let-loose-discord-bot",
  "version": "2.0.0",
  "description": "A comprehensive Discord bot for Hell Let Loose servers with VIP management, player linking, and contest system",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "dev": "nodemon bot.js",
    "test": "jest",
    "test:watch": "jest --watch",
